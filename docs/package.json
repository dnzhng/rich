{
  "name": "rich-docs",
  "version": "0.0.1",
  "description": "Documentation site for Rich react library",
  "scripts": {
    "start": "node src/app.js & npm run watch-css",
    "build-css": "node-sass --include-path scss ../assets/scss/app.scss src/css/main.css",
    "watch-css": "nodemon -e scss -x 'npm run build-css'"
  },
  "repository": {
    "type": "git",
    "url": "git@github.ibm.com:zhangda/rich.git"
  },
  "private": true,
  "dependencies": {
    "babel-cli": "^6.6.5",
    "babel-loader": "^6.2.4",
    "browser-sync": "^2.11.2",
    "connect-history-api-fallback": "^1.2.0",
    "react": "^0.14.8",
    "react-hot-loader": "^1.3.0",
    "react-router": "^2.3.0",
    "webpack": "^1.12.14",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.10.0"
  },
  "devDependencies": {
    "node-sass": "^3.5.3",
    "nodemon": "^1.9.1"
  }
}
